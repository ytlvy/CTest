{
    "env": {
        "myDefaultIncludePath": ["${workspaceFolder}", "${workspaceFolder}/LeetCode/**"]
      },
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/LeetCode",
                "${workspaceFolder}/LeetCode/**"
            ],
            "forcedInclude": ["${workspaceFolder}/LeetCode/helper.h"],
            "defines": [],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-arm64"
        }
    ],
    "version": 4
}